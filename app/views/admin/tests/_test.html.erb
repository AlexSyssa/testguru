<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.public ? 'Да' : 'Нет' %></td>
  <td><%= test.timer %></td>
  <td align = "center"><%= test.questions.size %></td>
  <td><%= link_to t('.show'), admin_test_path(test) %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test) %></td>
  <td><%= button_to t('.delete'), admin_test_path(test), method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
